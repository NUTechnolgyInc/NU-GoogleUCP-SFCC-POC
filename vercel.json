{
    "version": 2,
    "name": "nu-googleucp-sfcc-poc",
    "builds": [
        {
            "src": "a2a/chat-client/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api",
            "destination": "/api/index.py"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/a2a/chat-client/$1"
        }
    ],
    "env": {
        "USE_SCAPI": "true"
    }
}