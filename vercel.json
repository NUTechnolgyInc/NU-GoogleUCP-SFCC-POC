{
    "version": 2,
    "functions": {
        "api/index.py": {
            "includeFiles": "apps/business_agent/**"
        }
    },
    "rewrites": [
        {
            "source": "/api",
            "destination": "/api/index.py"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "outputDirectory": "dist",
    "env": {
        "USE_SCAPI": "true"
    }
}