{
    "version": 2,
    "name": "nu-googleucp-sfcc-poc",
    "buildCommand": "cd a2a/chat-client && npm install && npm run build",
    "outputDirectory": "a2a/chat-client/dist",
    "rewrites": [
        {
            "source": "/api",
            "destination": "/api/index.py"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        }
    ],
    "env": {
        "USE_SCAPI": "true"
    }
}